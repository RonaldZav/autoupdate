{
    "name": "autoupdate",
    "version": "1.0.0",
    "description": "Automatic update your code from github in your vps",
    "author": "RonaldZav",
    "main": "main.js",
    "scripts": {
        "start": "node src/main.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "child_process": "1.0.2",
        "express": "4.19.2",
        "js-yaml": "4.1.0",
        "moment": "2.30.1"
    },
    "bin": {
        "autoupdate": "./src/main.js"
    },
    "pkg": {
        "scripts": "src/main.js",
        "targets": [
            "node18-linux-x64"
        ],
        "assets": [
            "!**/config.yml"
        ]
    }
}
